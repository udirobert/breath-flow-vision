# =================================================================
# SUPABASE CONFIGURATION
# =================================================================

# Supabase Project URL (replace with your project)
VITE_SUPABASE_URL=https://your-project.supabase.co

# Supabase Anon Key (replace with your key)
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# =================================================================
# AI SERVICES
# =================================================================

# Google Gemini API Key (replace with your key)
VITE_GOOGLE_GEMINI_API_KEY=your_google_gemini_api_key_here

# =================================================================
# APPLICATION CONFIGURATION
# =================================================================

# App Environment
NODE_ENV=development

# App Base URL
VITE_APP_URL=http://localhost:8080

# Enable debug mode
VITE_DEBUG=true

# Feature flags
VITE_ENABLE_CAMERA=true
VITE_ENABLE_AI_ANALYSIS=true
VITE_ENABLE_SOCIAL_FEATURES=false
VITE_ENABLE_IP_REGISTRATION=false

# =================================================================
# DEVELOPMENT SETTINGS
# =================================================================

# Development server port
PORT=8080

# Enable source maps in development
VITE_GENERATE_SOURCEMAP=true

